{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "Simple URL shortener with analytics",
  "main": "index.js",
  "author": "Rob Alonzi <alonzi.rob@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --mode development  --display-error-details --config webpack.server.js --watch",
    "dev:build-client": "webpack --mode development  --display-error-details --config webpack.client.js --watch",
    "prod": "npm-run-all --parallel prod:*",
    "prod:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "prod:build-server": "webpack --mode production  --config webpack.server.js",
    "prod:build-client": "webpack --mode production  --config webpack.client.js",
    "test": "mocha --compilers js:babel-core/register \"./{,!(node_modules)/**/}*.test.js\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "css-loader": "^0.28.11",
    "device-detector": "^1.0.2",
    "expect": "^22.4.3",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.1.0",
    "material-ui": "^1.0.0-beta.38",
    "mocha": "^5.0.5",
    "moment": "^2.21.0",
    "mongoose": "^5.0.11",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "serialize-javascript": "^1.4.0",
    "style-loader": "^0.20.3",
    "supertest": "^3.0.0",
    "webpack": "^4.2.0"
  },
  "devDependencies": {
    "webpack-cli": "^2.0.12",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
}
